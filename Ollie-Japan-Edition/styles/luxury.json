					"color": "#56453F"
				},
				{
					"name": "Contrast",
					"slug": "main",
					"color": "#2A241C"
				},
				{
					"name": "Contrast Accent",
					"slug": "main-accent",
					"color": "#9F897A"
				},
				{
					"name": "Base",
					"slug": "base",
					"color": "#FAF5F2"
				},
				{
					"name": "Base Accent",
					"slug": "secondary",
					"color": "#AD947B"
				},
				{
					"name": "tint",
					"slug": "tertiary",     
					"color": "#EFE0D7"
				},
				{
					"name": "Border Light",
					"slug": "border-light",
					"color": "#CAC1B2"
				},
				{
					"name": "Border Dark",
					"slug": "border-dark",
					"color": "#594B38"
				}
			]
		}
	},
	"styles": {
		"elements": {
			"button": {
				"spacing": {
					"padding": {
						"top": ".6em",
						"right": "1.4em",
						"bottom": ".6em",
						"left": "1.4em"
					}
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--jp-serif)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"fontWeight": "300",
			"lineHeight": "var(--wp--custom--line-height--loose)",
			"letterSpacing": "0.03em"
		}
	}
